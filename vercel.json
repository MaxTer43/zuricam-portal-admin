{

  "version": 2,

  "builds": [

    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build"
    }

  ],

  "routes": [

    {

      "src": "/(.*)",

      "dest": "/index.html",

      "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],

      "headers":{

        "Access-Control-Allow-Origin": "*"

      }

    }

  ]

}